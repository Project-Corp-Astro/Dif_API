{
  "name": "corp-astro-api",
  "version": "1.0.0",
  "description": "Backend API for Corp Astro Mobile application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "seed": "ts-node scripts/seed.ts",
    "migrate": "node scripts/run-migrations.js",
    "migrate:create": "ts-node scripts/create-migration.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test-fcm": "ts-node src/scripts/testFCMNotification.ts",
    "migrate-fcm": "ts-node src/scripts/migrateFCMTokens.ts",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@bull-board/express": "^5.8.0",
    "@supabase/supabase-js": "^2.38.4",
    "@types/helmet": "^0.0.48",
    "@types/multer": "^1.4.12",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.9.0",
    "bullmq": "^4.12.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.1.0",
    "pdfkit": "^0.17.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "socket.io": "^4.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.22",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.57",
    "@types/pdfkit": "^0.13.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
