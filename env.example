# API Configuration
NODE_ENV=development
PORT=3000

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# Authentication
JWT_SECRET=your-jwt-secret-key
JWT_EXPIRY=1h

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Astro Engine API
ASTRO_ENGINE_URL=https://api.astroengine.com/v1
ASTRO_ENGINE_KEY=your-astro-engine-api-key

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:19000,http://localhost:19001,http://localhost:19002

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Subscription plans
SUBSCRIPTION_PLAN_MONTHLY=monthly_subscription_id
SUBSCRIPTION_PLAN_YEARLY=yearly_subscription_id
SUBSCRIPTION_PLAN_LIFETIME=lifetime_subscription_id

# Webhook secrets for signature verification
APPLE_WEBHOOK_SECRET=your_apple_webhook_secret
GOOGLE_WEBHOOK_SECRET=your_google_webhook_secret
